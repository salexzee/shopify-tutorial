{% include 'password-signup' with {
  headline: section.settings.headline,
  bg_image: section.settings.background_image
} %}

{% schema %}
  {
    "name": "Password Signup",
    "settings": [
      {
        "id": "headline",
        "type": "text",
        "label": "Headline",
        "default": "Welcome to My Site!"
      },
      {
        "id": "background_image",
        "type": "image_picker",
        "label": "Background Image"
      }
    ]
  }
{% endschema %}